      <xs:element name="movieId" type="xs:int" minOccurs="0"/>
      <xs:element name="movieName" type="xs:string" minOccurs="0"/>
      <xs:element name="status" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Categories">
    <xs:sequence>
      <xs:element name="categoryDesc" type="xs:string" minOccurs="0"/>
      <xs:element name="categoryId" type="xs:int" minOccurs="0"/>
      <xs:element name="categoryName" type="xs:string" minOccurs="0"/>
      <xs:element name="createdDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="customerDetailsByCreatedBy" type="tns:CustomerDetails" minOccurs="0"/>
      <xs:element name="customerDetailsByLastModifiedBy" type="tns:CustomerDetails" minOccurs="0"/>
      <xs:element name="lastModifiedDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="price" type="xs:int" minOccurs="0"/>
      <xs:element name="status" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

